Config { font = "xft:Inconsolata:size=16"
       , bgColor = "black"
       , fgColor = "grey"
       , position = Top
       , border = NoBorder
       , commands = [ Run Battery ["-t","<left>% ","-L","25","-H","75","-h","green","-n","yellow","-l","red"] 10
                    , Run Date "%a, KW %V, %d.%m.%y - %H:%M" "date" 10
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{ %battery% | <fc=#ee9a00>%date%</fc>"
       }

