Config { font = "xft:Inconsolata:size=12"
       , bgColor = "black"
       , fgColor = "grey"
       , position = Top
       , border = NoBorder
       , commands = [ Run Date "%a, KW %V, %d.%m.%y - %H:%M " "date" 60
                , Run Battery ["-t", "Bat <left>%", "-L", "25", "-H", "75", "-h", "grey", "-n", "yellow", "-l","red"] 60
                , Run Memory ["-t", "Mem <usedratio>%"] 60
                , Run DiskU [("/", "SSD <free>")]
			["-L", "1", "-H", "10", "-m", "1", "-p", "3", "-h", "grey", "-n", "yellow", "-l", "red"] 60
                , Run StdinReader
                ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{ %disku% | %memory% | %battery% | %date%"
       }

